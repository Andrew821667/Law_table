{
  "extends": "google",
  "env": {
    "browser": false,
    "es6": true,
    "googleappsscript/googleappsscript": true
  },
  "plugins": ["googleappsscript"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": "warn",
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "max-len": ["error", { "code": 120, "ignoreUrls": true, "ignoreStrings": true }],
    "camelcase": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "no-console": "off",
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  },
  "globals": {
    "SpreadsheetApp": "readonly",
    "Logger": "readonly",
    "UrlFetchApp": "readonly",
    "PropertiesService": "readonly",
    "ScriptApp": "readonly",
    "CacheService": "readonly",
    "LockService": "readonly",
    "Utilities": "readonly",
    "Session": "readonly",
    "Browser": "readonly",
    "HtmlService": "readonly",
    "DriveApp": "readonly",
    "GmailApp": "readonly",
    "CalendarApp": "readonly"
  }
}
