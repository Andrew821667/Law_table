# Telegram Bot Token
# Получить: https://t.me/BotFather
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Google Sheets API Key (опционально)
# Для публичных таблиц можно оставить пустым или удалить эту строку
GOOGLE_API_KEY=

# Название листа (опционально)
# Если не указано, используется первый лист
SHEET_NAME=Судебные дела

# Порт для сервера (по умолчанию 3000)
PORT=3000

# Базовый URL для Mini App (без trailing slash)
# Используется для генерации ссылок на веб-приложение
BASE_URL=https://your-domain.com
